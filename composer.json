{
    "name": "mediamaskio/mediamask-php",
    "description": "Mediamask PHP API Client for dynamic image generation",
    "keywords": [
        "api",
        "image-generation",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://mediamask.io",
    "license": "mit",
    "authors": [
        {
            "name": "Gregor Vostrak",
            "homepage": "https://mediamask.io"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Mediamask\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Mediamask\\Test\\" : "test/" }
    }
}
